{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "dependencies": [
        {
            "name": "opencv",
            "default-features": false,
            "features": [
                "contrib"
            ]
        },
        {
            "name": "fltk",
            "default-features": false,
            "$comment": "FLUID only (potentially) needed during developement...",
            "features": [
                "opengl",
                "fluid"
            ]
        },
        "argparse"
    ],
    "builtin-baseline": "401578455faece3db28615775420fb0f22f9bd07",
    "vcpkg-configuration": {
        "overlay-ports": ["./overlay_ports"]
    }
}
